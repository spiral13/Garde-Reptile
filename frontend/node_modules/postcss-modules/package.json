{
  "name": "postcss-modules",
  "version": "0.4.1",
  "description": "PostCSS plugin to use CSS Modules everywhere",
  "main": "build/index.js",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "modules",
    "css modules",
    "components"
  ],
  "author": "Alexander Madyankin <alexander@madyankin.name>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/outpunk/postcss-modules.git"
  },
  "dependencies": {
    "css-modules-loader-core": "^1.0.0",
    "postcss": "^5.0.14",
    "string-hash": "^1.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "ava": "^0.11.0",
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "file-exists": "^1.0.0"
  },
  "scripts": {
    "pretest": "$(npm bin)/eslint src test",
    "test": "$(npm bin)/ava --require babel-core/register",
    "transpile": "$(npm bin)/babel src -d build",
    "prepublish": "npm run transpile",
    "postpublish": "rm -rf build && git push --follow-tags"
  }
}
